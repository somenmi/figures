{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "api/ratings.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/ratings",
            "dest": "/api/ratings"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}